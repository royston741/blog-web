<%- include('partials/header'); -%>

    <div class="container">
        <h1>Home</h1>
    </div>

    <%posts.forEach(post=>{%>
        <div class="posts">
            <h2>
                <%=post.title%>
            </h2>
            <div class="cont">
                <p>
                    <%=post.content.substring(0,100) + '....' %>
                </p>
                <a class="link" href="/posts/<%=post.title%>">Read more</a>
            </div>
        </div>
        <%})%>






            <%- include('partials/footer'); -%>